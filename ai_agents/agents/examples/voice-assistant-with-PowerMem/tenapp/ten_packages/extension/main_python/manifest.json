{
  "type": "extension",
  "name": "main_python",
  "version": "0.1.0",
  "dependencies": [
    {
      "type": "system",
      "name": "ten_runtime_python",
      "version": "0.11"
    },
    {
      "type": "system",
      "name": "ten_ai_base",
      "version": "0.7"
    }
  ],
  "package": {
    "include": [
      "manifest.json",
      "property.json",
      "**.tent",
      "**.py",
      "README.md",
      "tests/**"
    ]
  },
  "api": {
    "property": {
      "properties": {
        "greeting": {
          "type": "string"
        },
        "user_id": {
          "type": "string"
        },
        "agent_id": {
          "type": "string"
        },
        "enable_memorization": {
          "type": "bool"
        },
        "enable_user_memory": {
          "type": "bool"
        },
        "memory_save_interval_turns": {
          "type": "int32"
        },
        "memory_idle_timeout_seconds": {
          "type": "float64"
        }
      }
    }
  }
}